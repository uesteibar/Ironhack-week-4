<ul>

  <%= form_tag("/contacts", method: "get") do %>
    <%= label_tag(:initial, "Search for:") %>
    <%= text_field_tag(:initial) %>
    <%= submit_tag("Search") %>
  <% end %>

  <% @contacts.each do |contact| %>
  <li>
    <%= link_to contact.name, contact_path(contact) %>
  </li>
  <% end %>
</ul>
