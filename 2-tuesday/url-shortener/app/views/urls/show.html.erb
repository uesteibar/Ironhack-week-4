<h3><%= link_to @url.shortcut, url_path(@url) %>
  -
  <%= link_to @url.url, @url.url, target: "_blank" %></h3>
<p>
  <%= "visited #{@url.times_visited} times" %>
</p>

<p>
  <% if @url.visited_at.nil? %>
  <%= "never visited" %>
  <% else %>
  <%= "last visited at #{@url.visited_at}" %>
  <% end %>
</p>
